<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:s="using:MvvmDialogSample.Services"
             xmlns:vm="using:MvvmDialogSample.ViewModels"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:DataType="vm:MainWindowViewModel"
			 s:DialogService.Register="{Binding DialogServiceSample}"
			 x:Class="MvvmDialogSample.Views.DialogServiceView">
	<Grid RowDefinitions="*,Auto">
		<ListBox ItemsSource="{Binding DialogServiceSample.FileNames}" />
		<Button Grid.Row="1" Content="Select Files" Command="{Binding DialogServiceSample.SelectFilesCommand}" />
	</Grid>
</UserControl>
